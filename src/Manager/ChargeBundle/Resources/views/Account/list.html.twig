{% extends '::base.html.twig' %}
{% block main %}
<script type="text/javascript">
	document.title = '{{ title }}';
</script>
<style>
td
{
	word-wrap:break-word;
	word-break:break-all;
}
</style>
<div class="centercontent">
    <div class="pageheader">
			<h1 class="pagetitle">{{ title }}</h1>
        <ul class="hornav">
            <li class="current">
			<a href="{{ path('#') }}">{{ title }}</a>
			</li>
        </ul>
    </div>
    <!--pageheader-->
    <div id="contentwrapper" class="contentwrapper">
        {% include 'ManagerChargeBundle:Account:list_form.html.twig' %}
        <div class="contenttitle2">
            <h3>列表结果</h3>
        </div>
        <div id="query_result">
            {% include 'ManagerChargeBundle:Account:query_result.html.twig' %}
        </div>
        <img id="loaders" src="/images/loaders/loader6.gif" alt="">
        <div clear="all"></div>
    </div>
    <!--contentwrapper-->
</div>
<!--centercontent-->

<script type="text/javascript">
//需要传入下面js的变量定义
var GLOBAL_CONF = {
    "action_query" : "{{ query_url }}",
    "params_key" : { {% for key in params_key %}{{ key }}:0,{% endfor %} }
};
</script>
<script type="text/javascript" src="/bundles/techlogmanager/js/article/list.js"></script>

{% endblock %}
